{
    "name": "cordoval/console-app",
    "description": "phpspec specified console app",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Cordova",
            "email": "cordoval@gmail.com"
        }
    ],
    "require": {
        "aura/di": "2.*@dev",
        "aura/cli": "2.*@dev",
        "aura/dispatcher": "2.*@dev"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1@dev",
        "bossa/phpspec2-expect": "dev-master@dev",
        "coduo/phpspec-prepare-extension": "dev-cordova-namespace-other-fix@dev",
        "coduo/phpspec-data-provider-extension": "dev-cordoval-namespace-fix@dev",
        "coduo/phpspec-matcher-extension": "dev-patch-1@dev",
        "henrikbjorn/phpspec-code-coverage": "~0.2.0",
        "ciaranmcnulty/phpspec-typehintedmethods": "dev-master@dev",
        "cordoval/best-practice-extension": "~1.0@dev"
    },
    "autoload": {
        "psr-4": { "Cordoval\\": "src/" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:cordoval/phpspec-matcher-extension"
        },
        {
            "type": "vcs",
            "url": "git@github.com:cordoval/phpspec-data-provider-extension"
        },
        {
            "type": "vcs",
            "url": "git@github.com:cordoval/phpspec-prepare-extension"
        }
    ]
}
